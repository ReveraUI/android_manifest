<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="revera"
          fetch="https://github.com/ReveraUI"
          revision="refs/heads/thirteen" />
  
  <remote name="superior"
          fetch="https://github.com/SuperiorOS/"
          revision="refs/heads/thirteen" />

  <remote name="superior-gitlab"
          fetch="https://gitlab.com/superioros"
          revision="refs/heads/thirteen" />

  <project path="manifest" name="android_manifest" remote="revera" />

  <!-- Frameworks Repos -->
  <project path="frameworks/base" name="android_frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="revera" />

  <!-- Vendor Repos-->
  <project path="vendor/lineage" name="android_vendor_lineage" remote="revera" />

  <!-- Apps-->
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="revera" />
  <project path="packages/apps/SetupWizard " name="android_packages_apps_SetupWizard" remote="revera" />
  <project path="packages/apps/Launcher3" name="android_packages_apps_Launcher3" remote="revera" />
  <project path="packages/resources/ThemedIcons" name="android_packages_resources_ThemedIcons" remote="superior">
    <linkfile src="res" dest="packages/apps/Launcher3/res-icons" />
  </project>
  <project path="packages/apps/FaceUnlockService" name="android_packages_apps_FaceUnlockService" remote="superior" />
  <project path="packages/apps/Bromite" name="android_packages_apps_Bromite" remote="revera" />

  <!-- External repos -->
  <project path="external/faceunlock" name="android_external_faceunlock" remote="superior-gitlab" />
</manifest>
